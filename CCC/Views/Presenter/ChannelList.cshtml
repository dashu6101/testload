@{
    ViewBag.Title = "Channel List";
}

@Html.Partial("_CategoryList")

<script type="text/javascript">
    $('.thumbnail').click(function () {
        var CategoryId = $(this).attr('Id');
        if (CategoryId != null) {
            window.location.href = '/Participant/ChannelDetails/' + CategoryId;
        }
    });
</script>
