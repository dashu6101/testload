@model IEnumerable<DeliverLoad.Models.CategoryModel>

@{
    ViewBag.Title = "Subscription Details";
    Layout = "~/Views/Shared/_Layout2.cshtml";
}


<div class="col-lg-12">
    
        <div class="col-lg-2">
            @Html.Partial("_SideMenu")
        </div>
    <div class="col-lg-10">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <h3 class="panel-title">
                    Subcription Details
                </h3>
            </div>
            <div class="block">
                <div id="tabs">
                    <table class="table table-responsive datatable">
                        <tr class="odd">
                            <th>

                                Channel No
                            </th>
                            <th>
                                Channel Name
                               
                            </th>

                            <th>
                                Price
                            </th>

                            <th>
                                Join Date
                            </th>


                        </tr>

                        @if (Model != null)
                        {

                            foreach (var item in Model)
                            {
                        <tr>
                            <td>
                                @Html.DisplayFor(model => item.ChannelNo)
                               
                            </td>
                            <td>
                                @Html.DisplayFor(model => item.Name)
                            </td>
                            <td>
                                @Html.DisplayFor(model => item.Price)
                            </td>

                            <td>
                                @Html.DisplayFor(model => item.JoinedDate)
                            </td>
                        </tr>
                            }
                        }
                    </table>
                </div>
            </div>
        </div>
        </div>
    </div>
