            if (UserType == "A") {
                $('#Title').val('');

                $('#treeDemo').jstree({
                    "plugins": ["contextmenu"]

                });
            }
            else {

                $("#treeDemo").jstree({
                    "plugins": ["sort", "unique"]
                });

            }

