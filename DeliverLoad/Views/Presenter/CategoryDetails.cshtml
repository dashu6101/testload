                    form_data.append("File", file_data);
                    form_data.append("VideoFrom", VideoFrom);
                    form_data.append("IsTreeView", false);

                    $.ajax({
                        url: '/TreeView/SaveTreeNode',
                        type: 'POST',
                        dataType: 'JSON',
                        cache: false,
                        contentType: false,
                        processData: false,
                        data: form_data,
                        success: function (data) {

                            if ($.isNumeric(data) == true) {
