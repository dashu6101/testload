@model DeliverLoad.Models.FindSpaceViewModel 
<div class="findspace">
    @using (Html.BeginForm("Index", "Vehicleowner", FormMethod.Post, new {@id="findspace", @class = "form-inline" }))
    {
        <div class="form-group col-sm-3">
            @Html.TextBoxFor(model => model.from, new { @id = "from", @class = "form-control", @placeholder = "From", @onfocus = "geolocate()" })
        </div>
        <div class="form-group col-sm-3">
            @Html.TextBoxFor(model => model.to, new { @id = "to", @class = "form-control", @placeholder = "To", @onfocus = "geolocate()" })
        </div>
        <div class="form-group col-sm-3">
            @Html.TextBoxFor(model => model.date, new { @id = "date", @class = "form-control", @placeholder = "Date" })
        </div>
        @Html.HiddenFor(model => model.isauthenticated, false)
        <div class="col-sm-3 text-center">
            <button type="submit" class="btn btn-theme btn-green">Find Space</button>
        </div>
    }
</div>
    <script>
        $(function () {
            $("#date").datepicker();
        });
    </script>